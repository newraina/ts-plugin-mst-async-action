  "keywords": [
    "Mobx",
    "mobx-state-tree",
    "TypeScript",
    "plugin"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc",
    "prepare": "npm run build",
    "test": "jest"
  },
  "prettier": {
